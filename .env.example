# Copy this file to .env and update the values to enable live AI conversations.
# Leaving API_ENDPOINT or API_KEY blank will keep the game in fallback dialogue mode.

AI_API_ENDPOINT=https://api.openai.com/v1/chat/completions
AI_API_KEY=your-openai-api-key
AI_MODEL=gpt-3.5-turbo
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=150
